            <kind>1</kind>
            <string>pulse += 0.01;
if (pulse &gt; 1)
    pulse = 0;
    
var up_pressed = false;
var down_pressed = false;
var confirm_pressed = false;
    
if (!scr_fading()) {
    up_pressed = keyboard_check_pressed(vk_up) || global.stick_up;
    down_pressed = keyboard_check_pressed(vk_down) || global.stick_down;
    confirm_pressed = keyboard_check_pressed(ord('A')) || gamepad_button_check_pressed(0, gp_face1);
}
    
if (up_pressed) {
    audio_play_sound(snd_select, 0, false);
    option--;
}
if (down_pressed) {
    audio_play_sound(snd_select, 0, false);
    option++;
}
    
if (option &lt; 0)
    option = ds_list_size(option_list) - 1;
if (option &gt; ds_list_size(option_list) - 1)
    option = 0;

if (confirm_pressed) {
    audio_stop_sound(snd_complete_music);
    audio_play_sound(snd_confirm, 0, false);
    switch (option) {
        case 0:
            scr_room_goto(global.next_room);
            break;
        case 1:
            scr_room_goto(rm_title);
            break;
    }
}
</string>
