draw_set_valign(fa_middle);

var text_size = 1 + 0.05 * sin(pulse * 2 * pi);

draw_set_font(fnt_title);
draw_text_transformed(room_width / 2, room_height / 5, "Into the Void", text_size, text_size, 0);

draw_set_font(fnt_info);

var separation = 24;
var arrow_width = 96;
var origin = room_height * (3 / 5) - (ds_list_size(option_list) - 1) * separation / 2;

for (var i = 0; i &lt; ds_list_size(option_list); i++) {
    var text = ds_list_find_value(option_list, i);
    switch (i) {
        case 1:
            text += ": ";
            if (window_get_fullscreen())
                text += "On";
            else
                text += "Off";
        break;
        case 2:
            text += ": ";
            if (global.sound_on)
                text += "On";
            else
                text += "Off";
        break;
        case 3:
            text += ": ";
            if (global.music_on)
                text += "On";
            else
                text += "Off";
        break;
        case 4:
            text += ": ";
            if (global.shake_high)
                text += "High";
            else
                text += "Low";
        break;
    }
    
    var scale = 1;
    if (option == i)
        scale = 1.5;
        
    draw_text_transformed(room_width / 2, origin + i * separation, text, text_size * scale, text_size * scale, 0);
}
