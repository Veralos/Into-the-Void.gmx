    audio_play_sound(snd_confirm, 0, false);
    switch (option) {
        case 0:
            scr_room_goto(room_next(room));
            break;
        case 1:
            window_set_fullscreen(!window_get_fullscreen());
            break;
        case 2:
            global.sound_on = !global.sound_on;
            break;
        case 3:
            global.music_on = !global.music_on;
            break;
        case 4:
            global.shake_high = !global.shake_high;
            break;
        case 5:
            game_end();
            break;
    }
}
