        <arguments>
          <argument>
            <kind>1</kind>
            <string>pulse += 0.01;
if (pulse &gt; 1)
    pulse = 0;
    
var up_pressed = false;
var down_pressed = false;
var confirm_pressed = false;

if (!scr_fading()) {
    up_pressed = keyboard_check_pressed(vk_up);
    down_pressed = keyboard_check_pressed(vk_down);
    confirm_pressed = keyboard_check_pressed(ord('A'));
}
    
if (up_pressed) {
    audio_play_sound(snd_select, 0, false);
    option--;
}
if (down_pressed) {
    audio_play_sound(snd_select, 0, false);
    option++;
}
    
if (option &lt; 0)
    option = ds_list_size(option_list) - 1;
if (option &gt; ds_list_size(option_list) - 1)
    option = 0;

if (confirm_pressed) {
    audio_play_sound(snd_confirm, 0, false);
    switch (option) {
        case 0:
            scr_room_goto(room_next(room));
            break;
        case 1:
            window_set_fullscreen(!window_get_fullscreen());
            break;
        case 2:
            global.sound_on = !global.sound_on;
            if (global.sound_on)
                scr_list_gain(sound_list, 1);
            else
                scr_list_gain(sound_list, 0);
            break;
        case 3:
            global.music_on = !global.music_on;
            if (global.music_on)
                scr_list_gain(music_list, 1);
            else
                scr_list_gain(music_list, 0);
            break;
        case 4:
            global.shake_high = !global.shake_high;
            break;
        case 5:
            game_end();
            break;
    }
}
</string>
